{
    "name": "chris1278/groupicons",
    "type": "phpbb-extension",
    "description": "This extension adds icons to the groups. These are then displayed in the legend next to the groups.",
    "homepage": "https://christian-esch.de",
    "version": "1.0.0",
    "time": "2022-08-12",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Chris1278",
            "email": "smilie-man@gmx.de",
            "homepage": "https://christian-esch.de",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0,<9.0.0",
        "phpbb/phpbb": ">=3.2.10,<3.4.0@dev",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "Icons for the groups in the legend view",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.10,<3.4.0@dev"
        }
		,
		"version-check": {
			"host": "raw.githubusercontent.com",
			"directory": "/chris1278/groupicons/main",
			"filename": "groupicons.json",
			"ssl": true
		}
    }
}
